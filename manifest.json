{
    "name": "The Great Suspender",
    "version": "4.80",
    "permissions": [
        "tabs",
        "storage",
        "unlimitedStorage",
        "http://*/*",
        "https://*/*",
        "chrome://favicon/*"
    ],
    "background": {
        "scripts": ["gsStorage.js", "background.js"]
    },
    "browser_action": {
        "default_title": "The Great Suspender",
        "default_icon": "icon128.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    },
    "web_accessible_resources": ["suspended.html", "history.html"],
    "incognito": "split",
    "manifest_version": 2,
    "minimum_chrome_version": "20"
}